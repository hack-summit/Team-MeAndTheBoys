                    <option value="indipop.mp3">I am Urban</option>
                    <option value="mn.mp3">Punjabi Mix</option>
                    <option value="punjabi.mp3">Prada</option>
                </select>
            </form>

        </div>
    </div>

    <script src="https://code.jquery.com/jquery-1.11.1.js"></script>
    <script src="https://code.jquery.com/jquery-1.11.1.js"></script>
    <script src="/js/chat.js"></script>
