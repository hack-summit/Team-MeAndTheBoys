    <link rel="stylesheet" href="/css/chat.css" type="text/css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.2.0/socket.io.dev.js"></script>
</head>

<body>
    <div class="container">
        <div class="top">
            <div class="content">
                <div class="pic">

                </div>

                <div class="name">

                </div>

                <div class="menu">

                </div>

            </div>

        </div>

        <div class="prev-song">

        </div>

        <div class="chat-screen">
